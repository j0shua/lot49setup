{
  "host":"va.opencmo.example.com",
  "port":10000,
  "cookieDomain":".opencmo.example.com",
  "baseUrl":"http://va.opencmo.example.com",
  "statsUrl":"http://b.opencmo.example.com",
  "keepAliveTimeoutSeconds":62,
  "winTimeoutSeconds":60,
  "timeoutThresholdMillis":35,
  "removeTagOnFirstNurlRequest":false,
  "orchestrator":{
    "deployType":"aws",
    "tagName":"bidderElb",
    "lot49ElbName":"lot49-us-east",
    "bElbName":"b-us-east",
    "awsFallbackNumberOfPeers":3,
    "awsDefaultTtlPeerCheckMinutes":5
  },
  "admin":{
    "username":"opencmo",
    "password":"password",
    "testDataDir":"/Users/admin/g/git/Lot49/test/data/"
  },
  "databases":{
    "aerospikes":{
      "aeroUserAttributes":{
        "enabled":false,
        "host":"127.0.0.1",
        "namespace":"test",
        "set":"uatts",
        "port":3000
      },
      "aeroUserSegments":{
        "enabled":false,
        "host":"127.0.0.1",
        "namespace":"test",
        "set":"usegs",
        "port":3000
      }
    },
    "redises":{
      "shortTermRedis":{
        "host":"user10.va.opencmo.example.com",
        "port":6379,
        "poolTtlMinutes":15,
        "poolSize":32
      }
    },
    "dynamodbs":{
      "dynUserSegments":{
        "table":"ucache",
        "keyField":"key"
      },
      "dynUserAttributes":{
        "table":"uatts",
        "keyField":"key"
      }
    }
  },
  "maxMind":{
    "city":"/opt/lot49/data/geo/maxmind/GeoIP2-City.mmdb",
    "connectionType":"/opt/lot49/data/geo/maxmind/GeoIP2-Connection-Type.mmdb",
    "domain":"/opt/lot49/data/geo/maxmind/GeoIP2-Domain.mmdb",
    "isp":"/opt/lot49/data/geo/maxmind/GeoIP2-ISP.mmdb"
  },
  "providers":{
    "ipblacklist":{
      "filePath":"/opt/lot49/data/ip/integral/list.txt.gz",
      "enabled":true,
      "initAsync":true
    }
  },
  "executorThreadPoolSize":32,
  "userIdCookie":"odsp",
  "jettyTracing":false,
  "experimentDataTtlDays":30,
  "userSegmentsCache":{
    "segmentsDb":"dynUserSegments"
  },
  "userAttributesCache":{
    "mapDb":"dynUserAttributes"
  },
  "adCache":{
    "ttlMinutes":1,
    "dir":"/opt/lot49/ads",
    "validateBudgetBeforeLoadingAd":true,
    "shortTermDb":"shortTermRedis",
    "domainListMaxSize":64000,
    "pacing":{
      "redis":{
        "host":"pace.va.opencmo.example.com",
        "port":6379,
        "timeoutMillis":1000,
        "fcTtlMilliseconds":100
      },
      "winRateTtlMinutes":60,
      "messageTtlMinutes":480,
      "winRateMin":0.01,
      "effectiveWinRateIfLessThanMin":0.1
    }
  },
  "integral":{
    "host":"http://integral.example.com",
    "port":8080,
    "maxConnPerRoute":1000,
    "clientId":"49600"
  },
  "clients":{
    "136":{
      "packageName":"com.client1.example.com",
      "dir":"/opt/lot49/client1",
      "subscriptions":[
        "INTEGRAL"
      ]
    }
  },
  "exchanges":{
    "openx":{
      "encryptionKey":"${env:LOT49_OPENX_ENCRYPTION_KEY}",
      "integrityKey":"${env:LOT49_OPENX_INTEGRITY_KEY}",
      "winTimeout":600
    },
    "adx":{
      "geoTable":"/opt/lot49/data/geo/adx/geo-table.csv",
      "encryptionKey": ${env:LOT49_ADX_ENCRYPTION_KEY},
      "integrityKey": ${env:LOT49_ADX_INTEGRITY_KEY},
      "nid":"${env:LOT49_ADX_NID}"
    },
    "bidswitch":{
      "seatId":"${env:LOT49_BIDSWITCH_SEAT_ID}"
    },
    "liverail":{
      "seatId":"${env:LOT49_LIVERAIL_SEAT_ID}"
    },
    "adaptv":{
      "assumeSwfIfVpaid":true,
      "defaultMaxDuration":30,
      "buyerId":"${env:LOT49_ADAPTV_BUYER_ID}"
    },
    "pubmatic":{
      "vcode":"${env:LOT49_PUBMATIC_VCODE}"
    }
  },
  "securityManagerConfig":{
    "packageBlackList":[
      "com.enremmeta.rtb.caches",
      "com.enremmeta.rtb.cli",
      "com.enremmeta.rtb.config",
      "com.enremmeta.rtb.dao",
      "com.enremmeta.rtb.impl",
      "com.enremmeta.rtb.jersey",
      "com.enremmeta.rtb.magic"
    ],
    "packageWhiteList":[

    ],
    "classBlackList":[
      "java.lang.Thread"
    ],
    "classWhiteList":[

    ],
    "networkAllowedMethodsMap":{
      "com.enremmeta.rtb.api.AdImpl":[
        "setTargetingUrlsFromUrl",
        "setDomainBlacklistFromUrl"
      ]
    }
  }
}
